{
  "name": "what-to-eat-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:system": "playwright test tests/system",
    "test:semantic-search": "ts-node --transpile-only scripts/test-semantic-search.ts",
    "setup:test-env": "node scripts/setup-test-env.js",
    "test:run-semantic": "npm run setup:test-env && vitest run tests/integration/semantic-search.test.ts",
    "test:run-migration": "node scripts/execute-sql.js",
    "generate-docs": "ts-node src/utils/docGenerator.ts",
    "generate-design-tokens-docs": "ts-node -e \"import('./src/utils/docGenerator.ts').then(m => m.generateDesignTokensDocs())\"",
    "generate-events-docs": "ts-node -e \"import('./src/utils/docGenerator.ts').then(m => m.generateEventSystemDocs())\"",
    "generate-observability-docs": "ts-node -e \"import('./src/utils/docGenerator.ts').then(m => m.generateObservabilityDocs())\""
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.1.0",
    "@supabase/supabase-js": "^2.49.5",
    "@tanstack/react-query": "^5.17.15",
    "@tanstack/react-virtual": "^3.0.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.8.4",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "https-proxy-agent": "^7.0.6",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.0.4",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.309.0",
    "mitt": "^3.0.1",
    "next": "14.0.4",
    "next-themes": "^0.4.6",
    "openai": "^4.100.0",
    "react": "^18",
    "react-dom": "^18",
    "react-i18next": "^15.4.1",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.2.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.42.0",
    "@testing-library/react": "^14.1.2",
    "@types/classnames": "^2.3.0",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "glob": "^11.0.1",
    "jsdom": "^22.1.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^1.2.1"
  }
}
